{
    "name": "jennosgroup/laratables",
    "description": "A laravel package that allows you to seemlessly generate html tables entirely using PHP classes.",
    "license": "MIT",
    "authors": [
        {
            "name": "Jenry Ollivierre",
            "email": "jenry@jenryollivierre.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Laratables\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laratables\\Providers\\AppServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
